{
  "name": "fbacks/yii2-api-template",
  "description": "Yii 2 REST API Template",
  "readme": "README.md",
  "keywords": [
    "yii",
    "yii2",
    "framework",
    "api",
    "rest",
    "project template",
    "MIT",
    "Feedback Software"
  ],
  "homepage": "http://fbacks.com/",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/FeedbackSoftware/yii2-api-template/issues?state=open",
    "source": "https://github.com/FeedbackSoftware/yii2-api-template"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.35"
  },
  "require-dev": {
    "yiisoft/yii2-gii": "^2.2"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
